<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="datos.Transporte" table="Transporte">
		<id column="idTransporte" name="idTransporte">
			<generator class="identity"/>
		</id>
		<property column="linea" name="linea" type="string"/>
		<property name="tipoTransporte" >
			<type name="org.hibernate.type.EnumType">
                <param name="enumClass">datos.TipoTransporte</param>
				<param name="type">4</param> <!-- 12 significa que es 
				un varchar esta miercoles -->
			</type>
		</property>
	</class>
</hibernate-mapping>
