<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="datos.Lectora" table="lectora">
		<id column="idLectora" name="idLectora">
			<generator class="identity" />
		</id>
		<property column="numeroSerieLectora" name="numeroSerieLectora" type="int"/>
		<joined-subclass name="datos.LectoraEstacion" table="LectoraEstacion">
			<key column="idLectora" />
			<many-to-one column="idEstacion" name="estacion" class="datos.Estacion" not-null="true" fetch="join"/>
		</joined-subclass>
		<joined-subclass name="datos.LectoraColectivo" table="LectoraColectivo">
			<key column="idLectora" />
			<many-to-one column="idTransporte" name="transporte" class="datos.Transporte" not-null="true" fetch="join"/>
		</joined-subclass>
		<joined-subclass name="datos.LectoraCarga" table="LectoraCarga">
			<key column="idLectora" />
			<many-to-one column="idEstacion" name="estacion" class="datos.Estacion" not-null="true" fetch="join"/>
		</joined-subclass>
		<joined-subclass name="datos.LectoraAutonoma" table="LectoraAutonoma">
			<key column="idLectora" />
			<many-to-one column="idEstacion" name="estacion" class="datos.Estacion" not-null="true" fetch="join"/>
			<property column="ubicacion" name="ubicacion" type="string"></property>
		</joined-subclass>
	</class>
</hibernate-mapping>
