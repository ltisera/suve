<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="datos.Movimiento" table="Movimiento">
		<id column="idMovimiento" name="idMovimiento">
			<generator class="identity"/>
		</id>
		<property column="fecha" name="fecha" type="calendar"/>
		<property column="monto" name="monto" type="float"/>
		
		<many-to-one column="idTarjeta" name="tarjeta" class="datos.Tarjeta" not-null="false"/>
