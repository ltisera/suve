<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="datos.Usuario" table="usuario">
		<id column="idUsuario" name="idUsuario">
			<generator class="identity" />
		</id>
		<property column="nombre" name="nombre" type="string" />
		<property column="apellido" name="apellido" type="string" />
		<property column="dni" name="dni" type="int" />
		<property column="mail" name="mail" type="string" />
		<property column="password" name="password" type="string" />
		<property name="tipoUsuario" >
			<type name="org.hibernate.type.EnumType">
                <param name="enumClass">datos.TipoUsuario</param>
				<param name="type">4</param>
			</type>
		</property>
		 
	</class>
</hibernate-mapping>