<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="datos.Tarjeta" table="Tarjeta">
		<id column="idTarjeta" name="idTarjeta">
			<generator class="identity" />
		</id>
		<property column="monto" name="monto" type="float" />
		<property column="activa" name="activa" type="boolean" />
		<many-to-one column="idUsuario" name="usuario" class="datos.Usuario" not-null="false" />

		<set table="tarjetaxbeneficio" name="beneficios" outer-join="true">
			<key column="idTarjeta" />
			<many-to-many column="idBeneficio" class="datos.Beneficio" />
		</set>

	</class>
</hibernate-mapping>
