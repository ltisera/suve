<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="datos.Lectora" table="lectora">
		<id column="idLectora" name="idLectora">
			<generator class="identity" />
		</id>
		<joined-subclass name="datos.LectoraTrenYSubte" table="LectoraTrenYSubte">
			<key column="idLectora" />
			<property column="realizaCarga" name="realizaCarga" type="boolean" />
			<many-to-one column="idEstacion" name="estacion" class="datos.Estacion" not-null="true" />
		</joined-subclass>
		<joined-subclass name="datos.LectoraColectivo" table="LectoraColectivo">
			<key column="idLectora" />
			<many-to-one column="idTransporte" name="transporte" class="datos.Transporte" not-null="true" />
		</joined-subclass>
	</class>
</hibernate-mapping>
