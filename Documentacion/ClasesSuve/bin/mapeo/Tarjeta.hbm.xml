<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="datos.Tarjeta" table="Tarjeta">
	<id column="idTarjeta" name="idTarjeta">
		<generator class="identity"/>
	</id>
		<property column="saldo" name="saldo" type="float"/>
		<property column="tarifaSocial" name="tarifaSocial" type="boolean"/>
		<property column="boletoEstudiantil" name="boletoEstudiantil" type="boolean"/>
		
		<many-to-one name="Usuario" class="datos.Usuario" column="idUsuario" not-null="true"/>
		<many-to-one name="Boleto" class="datos.Boleto" column="idBoleto" not-null="true"/>
	</class>
</hibernate-mapping>
