<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

<class name="datos.Boleto" table="Boleto">

	<id column="idBoleto" name="idBoleto">
		<generator class="identity"/>
	</id>
	
	<property column="fecha" name="fecha" type="calendar"/>
	<property column="precio" name="precio" type="float"/>
	<property column="cerrado" name="cerrado" type="boolean"/>
	
	<set name="Tarjeta" table="Tarjeta" order-by="idTarjeta asc" inverse="true" lazy="true" fetch="select">
		<key column ="idBoleto" not-null="true"/>
		<one-to-many class="datos.Tarjeta"/>
	</set>
	
</class>
</hibernate-mapping>