<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="datos.Boleto" table="Boleto">
		<id column="idBoleto" name="idBoleto">
			<generator class="identity"/>
		</id>
		<property column="fecha" name="fecha" type="calendar"/>
		<property column="precio" name="precio" type="float"/>
		<property column="cerrado" name="cerrado" type="boolean"/>
		<many-to-one column="idTransporte" name="transporte" class="datos.Transporte" not-null="true"/>
		<many-to-one column="idTarjeta" name="tarjeta" class="datos.Tarjeta" not-null="true"/>
		<many-to-one column="estacionSubida" name="estacionSubida" class="datos.Estacion" not-null="false"/>
		<many-to-one column="estacionBajada" name="estacionBajada" class="datos.Estacion" not-null="false"/>
		</class>
</hibernate-mapping>